<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="amd.example.demo">

    <application
        android:name="amd.example.java.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Java">

        <activity android:name="amd.example.java.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="amd.example.java.view.ViewPagerTwoActivity" />

        <activity
            android:name="amd.example.java.view.FaceUnityActivity"
            android:exported="true" />
        <activity android:name="amd.example.java.view.MVPActivity" />
        <activity android:name="amd.example.java.view.AgoraActivity" />
    </application>

</manifest>